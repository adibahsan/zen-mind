{
  "expo": {
    "name": "zen mind",
    "slug": "zen-mind",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/zen-loading.svg",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/zen-loading.svg",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "c88856f7-189a-4606-a26f-6cd69168c505"
      }
    },
    "android": {
      "package": "com.adibahsan.zenmind"
    }
  }
}
